= render "shared/header"
.wrapper
  .content
    .user__mypage
      .user__mypage__sidebar
        .user__mypage__sidebars
          %ul.user__mypage__sidebars
            %li.user__mypage__sidebars__top
              = link_to '/jp/mypage/' do
                マイページ
                %i.fa.fa-chevron-right
            %li.user__mypage__sidebars__item
              = link_to '/jp/mypage/notification/' do
                お知らせ
                %i.fa.fa-chevron-right
            %li.user__mypage__sidebars__item
              = link_to '/jp/mypage/todo/' do
                やることリスト
                %i.fa.fa-chevron-right
            %li.user__mypage__sidebars__item
              = link_to '/jp/mypage/like/history/' do
                いいね！一覧
                %i.fa.fa-chevron-right
            %li.user__mypage__sidebars__item
              = link_to '/jp/sell/' do
                出品する
                %i.fa.fa-chevron-right
            %li.user__mypage__sidebars__item
              = link_to '/jp/mypage/listings/listing/' do
                出品した商品-出品中
                %i.fa.fa-chevron-right
            %li.user__mypage__sidebars__item
              = link_to '/jp/mypage/listings/completed/' do
                出品した商品-売却済み
                %i.fa.fa-chevron-right
            %li.user__mypage__sidebars__item
              = link_to '/jp/mypage/purchase/' do
                購入した商品-取引中
                %i.fa.fa-chevron-right
            %li.user__mypage__sidebars__item
              = link_to '/jp/mypage/purchased/' do
                購入した商品-過去の取引
                %i.fa.fa-chevron-right
            %li.user__mypage__sidebars__item
              = link_to '/jp/mypage/news/' do
                ニュース一覧
                %i.fa.fa-chevron-right
            %li.user__mypage__sidebars__item
              = link_to '/jp/mypage/review/history/' do
                評価一覧
                %i.fa.fa-chevron-right
            %li.user__mypage__sidebars__item
              = link_to '/jp/help_center/' do
                ガイド
                %i.fa.fa-chevron-right
            %li.user__mypage__sidebars__item
              = link_to '/jp/mypage/support/' do
                お問い合わせ
                %i.fa.fa-chevron-right


          .user__mypage__sidebars__middle
            %pメルペイ
            %ul.user__mypage__sidebars
              %li.user__mypage__sidebars__item
                = link_to '/jp/mypage/sales/' do
                  売上
                  %i.fa.fa-chevron-right
              %li.user__mypage__sidebars__item
                = link_to '/jp/mypage/point/' do
                  ポイント
                  %i.fa.fa-chevron-right
          .user__mypage__sidebars__bottom
            %p設定
            %ul.user__mypage__sidebars
              %li.user__mypage__sidebars__item
                = link_to '/jp/mypage/profile/' do
                  プロフィール
                  %i.fa.fa-chevron-right
              %li.user__mypage__sidebars__item
                = link_to '/jp/mypage/deliver_address/' do
                  発送元・お届け先住所変更
                  %i.fa.fa-chevron-right
              %li.user__mypage__sidebars__item
                = link_to '/jp/mypage/card/' do
                  支払い変更
                  %i.fa.fa-chevron-right
              %li.user__mypage__sidebars__item
                = link_to '/jp/mypage/email_password/' do
                  メール/パスワード
                  %i.fa.fa-chevron-right
              %li.user__mypage__sidebars__item
                = link_to '/jp/mypage/identification/' do
                  本人情報
                  %i.fa.fa-chevron-right
              %li.user__mypage__sidebars__item
                = link_to '/jp/mypage/sms_confirmation/' do
                  電話番号の確認
                  %i.fa.fa-chevron-right
              %li.user__mypage__sidebars__item
                = link_to '/jp/logout/' do
                  ログアウト
                  %i.fa.fa-chevron-right


      .user__mypage__profile
        .user__mypage__profile__imagebox
          = link_to "https://www.mercari.com/jp/u/629853077/" do
            %img(src="https://static.mercdn.net/images/member_photo_noimage_thumb.png" alt="aa")/
            %h2.bold えりん
          .mypage__numbers
            %div
              評価
              %span.bold 0
            %div
              出品数
              %span.bold 0
        .user__mypage__profile__tabs
          %ul.profile__tabs
            %li.move
              %h3.side
                = link_to "#mypage-tab-notification" do
                  お知らせ
            %li.passive
              %h3.side
                = link_to "#mypage-tab-todo" do
                  やることリスト
        .user__mypage__profile__contents
          %ul.user__mypage__profile__contents
            %li.user__mypage__profile__contents__item

            %li.user__mypage__profile__contents__item
              %img(src="https://static.mercdn.net/images/mercari_profile.png" alt="aa")/
              増税
            %li.user__mypage__profile__contents__item
              P200
            %li.user__mypage__profile__contents__item
              KASHIMA
            %li.user__mypage__profile__contents__itemlist
              一覧をみる

          .user__mypage__profile__product
            .user__mypage__profile__product__tabs
              .user__mypage__profile__tabs__message
                お知らせ
              .user__mypage__profile__tabs__message
                過去の取引
            .user__mypage__profile__trade
              購入した商品
              .user__mypage__profile__trade__product
                取引中の商品がありません

= render "shared/footer"